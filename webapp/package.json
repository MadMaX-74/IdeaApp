{
  "name": "@ideaapp/webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "stylelint": "stylelint src/**/*.scss",
    "preview": "vite preview",
    "prettier": "prettier --log-level warn --cache --write '{src/**/*,*}.{js,jsx,ts,tsx, json, css, scss, yml}' '!**/{node_modules, dist}/**/*'",
    "typecheck": "tsc --noEmit --project ./tsconfig.app.json",
    "stylecheck": "sass --update -q ./src:./node_modules.cache/ ./stylecheck"
  },
  "dependencies": {
    "@ideaapp/server": "workspace:*",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "formik": "^2.4.6",
    "include-media": "^2.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.1",
    "reset-css": "^5.0.2",
    "sass": "^1.83.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "stylelint": "^16.12.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.5"
  }
}
